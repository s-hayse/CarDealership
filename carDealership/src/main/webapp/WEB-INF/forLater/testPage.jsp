const findBestMatch = name => {
  let bestMatch
  let bestMatchDistance = 99999999
  bodyTypeMappings.map(([tableName, taxonomy]) => {
    let _dist = levenshtein(name.toLowerCase(), tableName.toLowerCase())
    if (_dist < bestMatchDistance) {
      bestMatchDistance = _dist
      bestMatch = taxonomy
    }
  })
  return bestMatch
}

const groupByTaxonomy = async (data) => {
  bodyTypes = {}
  data.map(bt => {
    const taxonomy = findBestMatch(bt.key)
    if (!(taxonomy in bodyTypes)) {
      bodyTypes[taxonomy] = { names: [], total: 0 }
    }
    if (!bodyTypes[taxonomy]['names'].includes(bt.key)) {
      bodyTypes[taxonomy]['names'].push(bt.key)
      bodyTypes[taxonomy]['total'] += bt.doc_count
    }
  })
}


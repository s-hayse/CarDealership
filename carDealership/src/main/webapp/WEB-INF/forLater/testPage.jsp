  }
  const { includeCertified } = initialCategory;

  const availablePreowned = ['PREOWNED', 'PREOWNED CERTIFIED'];
  const allPreowned = conditions.filter(condition => availablePreowned.includes(condition.key));
  const totalPreowned = allPreowned.length
    ? allPreowned
      .map(condition => condition.doc_count)
      .reduce((a, b) => a + b)
    : 0;
  
  const excludeConditions = includeCertified
    ? ['PREOWNED']
    : availablePreowned

  const otherConditions = conditions
    .filter(condition => !excludeConditions.includes(condition.key))
    .map(condition => Object.assign({}, condition, { names: [condition.key] }))

  const newConditions = [
    ...otherConditions,
    {
      key: 'ALL PREOWNED',
      names: availablePreowned,
      doc_count: totalPreowned
    }
  ]

  return newConditions
}


function minOrMaxValue (value, key, defaultValues) {
  let newValue = value

  for (const fields of coupleFields) {
    if (fields.includes(key)) {
      newValue = Math.min(Math.max(parseInt(newValue), defaultValues[fields[0]]), defaultValues[fields[1]])
      break;
    }
  }

  if (key.endsWith('MonthlyPayment')) {
    newValue = Math.min(Math.max(parseInt(newValue), defaultValues.minMonthlyPayment), defaultValues.maxMonthlyPayment)
  }

  return newValue
}

function isRangeEqualToDefault (value, key, defaultValues) {
  for (const fields of coupleFields) {
    if (fields.includes(key) && defaultValues[key] === parseInt(value)) {
      return true
    }
  }

  if (key.endsWith('MonthlyPayment') && defaultValues[key] === parseInt(value)) {
    return true
  }

  return false
}


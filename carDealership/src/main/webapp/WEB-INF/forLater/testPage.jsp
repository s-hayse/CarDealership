          $(this).find('.est-payment').html(newTemplate)
        }
        else {
          const newTemplate = `${formatNumber(monthlyPayment, '$', '', 0)} /mo`
          $(this).find('.est-payment').html(newTemplate)
        }
      }
    })
  }


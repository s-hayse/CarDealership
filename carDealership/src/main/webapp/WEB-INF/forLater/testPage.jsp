const concatFilters = (filters, form) => {
  const { filterForm } = inventoryState.get()
  if (filterForm[form]) {
    const newForm = filterForm[form]
      .filter(formKey => {
        const found = filters.some(filter => filter.key === formKey)
        return !found
      })

    newForm.map(addNew => {
      filters.push({
        key: addNew,
        doc_count: 0
      })
    })
  }

  return filters
}

const updateAvailableFilters = async (newFilters) => {
  const {
    bodyTypes,
    driveTypes,
    exteriorColors,
    interiorColors,
    inventoryLocations,
    makes,
    miles,
    models,
    price,
    transmissions,
    trims,
    vehicles,
    years,
    fuelTypes
  } = newFilters

  const { filters, filterForm, aggregatedFilters } = inventoryState.get()

  // change filters with the catergory result
  filters.maxPrice = price.max   
  filters.minPrice = price.min
  filters.maxMiles = miles.max
  filters.minMiles = miles.min
  filters.maxYear = years.max
  filters.minYear = years.min

  filters.makes = concatFilters(makes, 'make')
  filters.makes = filters.makes.sort(compareFilterKey)
  filters.makes.unshift({
    key: 'Any',
    doc_count: filters.makes.length ? filters.makes.map(x => x.doc_count).reduce((a, b) => a + b) : 0
  })

  filters.models = concatFilters(models, 'model')
  filters.models = filters.models.sort(compareFilterKey)
  filters.models.unshift({
    key: 'Any',
    doc_count: filters.models.length ? filters.models.map(x => x.doc_count).reduce((a, b) => a + b) : 0
  })

  const extColors = aggregateScarce(concatFilters(exteriorColors, 'exteriorColor'))
  aggregatedFilters.exteriorColor = { list: extColors.others, real: extColors.items }
  filters.exteriorColors = extColors.items

  const intColors = aggregateScarce(concatFilters(interiorColors, 'interiorColor'))
  aggregatedFilters.interiorColor = { list: intColors.others, real: intColors.items }
  filters.interiorColors = intColors.items

  const trimOther = aggregateScarce(concatFilters(trims, 'trim'))
  filters.trims = trimOther.items.sort(compareFilterKey)
  filters.trims.unshift({
    key: 'Any',
    doc_count: filters.trims.length ? filters.trims.map(trim => trim.doc_count).reduce((total, currentValue) => total + currentValue) : 0
  })
  aggregatedFilters.trim = { list: trimOther.others, real: trimOther.items }

  filters.bodies = await groupByBodyType(bodyTypes)

  const driveTrain = aggregateScarce(concatFilters(driveTypes, 'driveTrain'))
  aggregatedFilters.driveTrain = { list: driveTrain.others, real: driveTrain.items }
  filters.drivetrains = driveTrain.items

  const transmission = aggregateScarce(concatFilters(transmissions, 'transmission'))
  aggregatedFilters.transmission = { list: transmission.others, real: transmission.items }
  filters.transmissions = transmission.items

  const fuelType = aggregateScarce(concatFilters(fuelTypes, 'fuelType'))
  aggregatedFilters.fuelType = { list: fuelType.others, real: fuelType.items }
  filters.fuelTypes = fuelType.items

  if (inventoryLocations) {
    filters.inventoryLocations = concatFilters(inventoryLocations, 'inventoryLocation')
  }

  filters.conditions = groupConditions(concatFilters(vehicles, 'vehicleCondition'))

  inventoryState.set({ filters })
  document.dispatchEvent(new CustomEvent("renderFilters"))
}

function serializeQuery (queries, removeKeys = []) {
  let normalizedQuery = ''
  let append = '&'
  let keyNames = Object.keys(queries)
  Object.entries(queries)
    .filter(([key, val]) => !removeKeys.includes(key))
    .sort()
    .map(([key, val]) => {
      if (Array.isArray(val)) {
        const encoded = val.map(elem => encodeURIComponent(elem))
        append += key + '=' + encoded.join(';') + '&'
      }
      else{
        append += key + '=' + encodeURIComponent(val) + '&'
      }
    })

  normalizedQuery = append.slice(1, append.length - 1)
  // for seo we need to remove spaces in query params
  const newUrl = normalizedQuery ? normalizedQuery.replace(/\%20/g,'_') : normalizedQuery
  return newUrl
}


const createEvents = (key, form, extraFunc) => {
  $(`#${key}`).change(function (event) {
    const { loading } = inventoryState.get()
    if (loading) {
      $(`#${key}`).prop('checked', false)
      return
    }
    changeArrayValues(event.target.value, form, this.checked)
    formHandler()
  })

  $(`#${key}m`).change(function (event) {
    changeArrayValues(event.target.value, form, this.checked)
    if (extraFunc) { extraFunc() }
  })
}

const createAnyEvents = (key, extraFunc) => {
  $(`#${key}Any`).change(function (event) {
    if (this.checked) {
      if (extraFunc) { extraFunc() }
      anyUpdateEvent(key)
      formHandler()
    }
  })
  $(`#${key}Anym`).change(function (event) {
    if (this.checked) {
      if (extraFunc) { extraFunc() }
      anyUpdateEvent(key)
    }
  })
}

const getItem = key => {
  const { filterForm, defaultFilters, filters } = inventoryState.get()
  return parseInt(filterForm[key] || (filters[key] || defaultFilters[key]))
}

const makeFunc = () => {
  const { filterForm } = inventoryState.get()
  delete filterForm.trim
  delete filterForm.model
  inventoryState.set({ filterForm })
}

const disableLocationInputs = () => {
  const { filterForm: { inventoryLocation } } = inventoryState.get()

  if (inventoryLocation && inventoryLocation.length) {
    inventoryLocation.map(item => {
      item = removeWhitespace(`inventoryLocation${item}`)
      $(`#${item}, #${item}m`).prop('disabled', (inventoryLocation.length === 1))
    })
  } 
}

const capitalizeCondition = str => {
  str = str.charAt(0).toUpperCase() + str.slice(1).toLowerCase()
  str = str.replace('Preowned', 'Pre-owned')

  return str
}

const disableConditionInputs = () => {
  const { filterForm: { vehicleCondition } } = inventoryState.get()
  
  if (
    !initialCategory.radioButtonFilters.includes('vehicleCondition')
    && vehicleCondition
    && vehicleCondition.length
  ) {
    vehicleCondition.map(item => {
      item = removeWhitespace(`vehicleCondition${item}`)
      $(`#${item}, #${item}m`).prop('disabled', (vehicleCondition.length === 1))
    })
  }
}

const addEvents = (filters) => {
  const {
    bodies,
    conditions, 
    drivetrains,
    exteriorColors,
    interiorColors,
    inventoryLocations,
    makes,
    models,
    transmissions,
    trims,
    fuelTypes
  } = filters

  makes.map(item => {
    if (item.key === '') return;
    const key = removeWhitespace(`make${item.key}`)

    if (key !== 'makeAny') {
      $(`#${key}, #${key}m`).change(function (event) {
        const { loading } = inventoryState.get()
        if (loading) {
          $(`#${key}`).prop('checked', false)
          return
        }
        makeFunc()
        changeArrayValues(event.target.value, 'make', this.checked)
        formHandler()
      })
    }
  })

  $(`#makeAny, #makeAnym`).change(function (event) {
    if (this.checked) {
      makeFunc()
      anyUpdateEvent('make')
      formHandler()
    }
  })

  models.map(item => {
    if (item.key === '') return;
    const key = removeWhitespace(`model${item.key}`)
    if (key !== 'modelAny') {
      createEvents(key, 'model')
    }
  })

  createAnyEvents('model')

  exteriorColors.map(color => {
    if (!(color.doc_count > 0)) {
      return
    }
    if (color.key === '') return;
    const key = removeWhitespace(`exteriorColor${color.key}`)
    createEvents(key, 'exteriorColor')
  })

  interiorColors.map(color => {
    if (!(color.doc_count > 0)) {
      return
    }
    if (color.key === '') return;
    const key = removeWhitespace(`interiorColor${color.key}`)
    createEvents(key, 'interiorColor')
  })

  trims.map(item => {
    if (item.key === '') return;
    const key = removeWhitespace(`trim${item.key}`)
    if (key !== 'trimAny') {
      createEvents(key, 'trim')
    }
  })

  createAnyEvents('trim')

  bodies.map(item => {
    if (item.key === '') return;
    const key = removeWhitespace(`bodyType${item.key}`)
    createEvents(key, 'bodyType')
  })

  drivetrains.map(item => {
    if (item.key === '') return;
    const key = removeWhitespace(`driveTrain${item.key}`)
    createEvents(key, 'driveTrain')
  })

  transmissions.map(item => {
    if (item.key === '') return;
    const key = removeWhitespace(`transmission${item.key}`)
    createEvents(key, 'transmission')
  })

  fuelTypes.map(item => {
    if (item.key === '') return;
    const key = removeWhitespace(`fuelType${item.key}`)
    createEvents(key, 'fuelType')
  })

  const filteredConditions = conditions.filter(item => item.doc_count > 0);
  if (filteredConditions.length > 1) {
    filteredConditions.map(item => {
      if (item.key === '') return;
      const key = removeWhitespace(`vehicleCondition${item.key}`)
      createEvents(key, 'vehicleCondition', disableConditionInputs)
    })
  }

  if (inventoryLocations && inventoryLocations.length > 1) {
    inventoryLocations.map(item => {
      if (item.key === '') return;
      const key = removeWhitespace(`inventoryLocation${item.key}`)
      createEvents(key, 'inventoryLocation', disableLocationInputs)
    })
  }
}

const setInputValues = () => {
  $('#desktopMinPriceFilter, #mobileMinPriceFilter').val(priceFormat.to(getItem('minPrice')))
  $('#desktopMaxPriceFilter, #mobileMaxPriceFilter').val(priceFormat.to(getItem('maxPrice')))
  document.getElementById('desktopPriceSlider').noUiSlider.set([getItem('minPrice'), getItem('maxPrice')])
  document.getElementById('mobilePriceSlider').noUiSlider.set([getItem('minPrice'), getItem('maxPrice')])

  $('#desktopMinMilesFilter, #mobileMinMilesFilter').val(milesFormat.to(getItem('minMiles')))
  $('#desktopMaxMilesFilter, #mobileMaxMilesFilter').val(milesFormat.to(getItem('maxMiles')))
  document.getElementById('desktopMilesSlider').noUiSlider.set([getItem('minMiles'), getItem('maxMiles')])
  document.getElementById('mobileMilesSlider').noUiSlider.set([getItem('minMiles'), getItem('maxMiles')])

  $('#desktopMinYearFilter, #mobileMinYearFilter').val(yearFormat.to(getItem('minYear')))
  $('#desktopMaxYearFilter, #mobileMaxYearFilter').val(yearFormat.to(getItem('maxYear')))
  document.getElementById('desktopYearSlider').noUiSlider.set([getItem('minYear'), getItem('maxYear')])
  document.getElementById('mobileYearSlider').noUiSlider.set([getItem('minYear'), getItem('maxYear')])

  $('#desktopMinMonthlyPaymentFilter, #mobileMinMonthlyPaymentFilter').val(priceFormat.to(getItem('minMonthlyPayment')))
  $('#desktopMaxMonthlyPaymentFilter, #mobileMaxMonthlyPaymentFilter').val(priceFormat.to(getItem('maxMonthlyPayment')))
  document.getElementById('desktopMonthlyPaymentSlider').noUiSlider.set([getItem('minMonthlyPayment'), getItem('maxMonthlyPayment')])
  document.getElementById('mobileMonthlyPaymentSlider').noUiSlider.set([getItem('minMonthlyPayment'), getItem('maxMonthlyPayment')])
}

const renderDesktopFilters = (filters) => {
  const {
    bodies,
    conditions, 
    drivetrains,
    exteriorColors,
    interiorColors,
    inventoryLocations,
    makes,
    models,
    transmissions,
    trims,
    fuelTypes
  } = filters

  resetFilterList('make')
  makes.map(item => {
    if (item.key === '') return;
    renderFilter(item, 'make')
  })

  resetFilterList('model')
  models.map(item => {
    if (item.key === '') return;
    renderFilter(item, 'model')
  })

  resetFilterList('exteriorColor')
  exteriorColors.map(color => {
    if (!(color.doc_count > 0) || color.key === '') {
      return
    }

    renderFilter(color, 'exteriorColor', true)
  })

  resetFilterList('interiorColor')
  interiorColors.map(color => {
    if (!(color.doc_count > 0) || color.key === '') {
      return
    }

    renderFilter(color, 'interiorColor', true)
  })

  resetFilterList('trim')
  trims.map(item => {
    if (item.key === '') return;
    renderFilter(item, 'trim')
  })

  createAnyEvents('trim')

  resetFilterList('bodyType')
  bodies.map(item => {
    if (item.key === '') return;
    renderFilter(item, 'bodyType')
  })

  resetFilterList('driveTrain')
  drivetrains.map(item => {
    if (item.key === '') return;
    renderFilter(item, 'driveTrain')
  })

  resetFilterList('transmission')
  transmissions.map(item => {
    if (item.key === '') return;
    renderFilter(item, 'transmission')
  })

  resetFilterList('fuelType')
  fuelTypes.map(item => {
    if (item.key === '') return;
    renderFilter(item, 'fuelType')
  })

  resetFilterList('vehicleCondition')
  $('#desktopConditionFilter, #conditionCollapse').html('')
  const filteredConditions = conditions.filter(item => item.doc_count > 0);
  if (filteredConditions.length > 1) {
    filteredConditions.map(item => {
      if (item.key === '') return;
      item.text = capitalizeCondition(item.key)
      renderFilter(item, 'vehicleCondition')
    })
  }

  resetFilterList('inventoryLocation')
  if (inventoryLocations && inventoryLocations.length > 1) {
    inventoryLocations.map(item => {
      if (item.key === '') return;
      renderFilter(item, 'inventoryLocation')
    })
  }
}

document.addEventListener("renderFilters", () => {
  const { filters } = inventoryState.get()
  renderDesktopFilters(filters)
  addEvents(filters)
  setInputValues()
})

document.addEventListener("initialRenderFilters", () => {
  const { filters } = inventoryState.get()
  createSliders()
  addEvents(filters)
  setInputValues()
  document.dispatchEvent(new CustomEvent('readQueryString'))
})


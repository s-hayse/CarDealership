    if (window._touchstartX - window._touchendX > 35) {
      scrollCarImages(carouselName, carId, 'next', index)
    } else if (window._touchstartX - window._touchendX < -35) {
      scrollCarImages(carouselName, carId, 'prev', index)
    }
  }
}


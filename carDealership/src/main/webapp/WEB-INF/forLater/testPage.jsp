const parseQueryString = qstr => {
  const result = {}
  let search = {}
  let params = qstr
  if (params.indexOf('?') === 0) {
    params = params.slice(1, params.length)
  }
  params = params.split('&')

  params.filter(x => x).map(p => {
    const k = decodeURIComponent(p.split('=')[0])
    let v = p.split('=')[1].replace(/_/g, ' ')
    // remove paymentTerms params from search form object
    if (k !== 'paymentTerms') {
      if (v.indexOf(';') >= 0) {
        v = v.split(';').map(x => decodeURIComponent(x))
      } else {
        v = decodeURIComponent(v)
        if (/^\d+$/.test(v)) {
          v = parseInt(v)
        }
      }

      // Validate bodyTypes
      if (k == 'bodyType' && Array.isArray(v)) {
        const { filters } = inventoryState.get()
        v = v.filter(bodyType =>
          filters.bodies.some(body => body.names.includes(bodyType))
        )
      }

      search[k] = v
    }
  })
  Object.entries(search).map(([k, v]) => {
    result[k] = _arrayFields.includes(k) && !Array.isArray(v) ? [v] : v
  })
  return result
}


const kFormatter = (num, symbol, end) => {
  const result = Math.abs(num) > 999 ? Math.sign(num) * ((Math.abs(num) / 1000).toFixed(1)) + 'k' : Math.sign(num) * Math.abs(num)
  if (end) return result + symbol
  return symbol + result
}

const updateArrayFilterForm = (item, key) => {
  const id = removeWhitespace(`${key}${item}`)
  const { filterForm } = inventoryState.get()

  $(`#${id}, #${id}m`).prop('checked', false)

  if (key === 'bodyType') {
    const { filters } = inventoryState.get()
    filters.bodies
      .filter(bodie => bodie.key === item)
      .map(body => {
        filterForm[key] = filterForm[key].filter(form => !body.names.includes(form))
      })
  } else if (item === 'Other') {
    const { aggregatedFilters } = inventoryState.get()
    filterForm[key] = filterForm[key].filter(item => !aggregatedFilters[key].list.includes(item))
  } else {
    filterForm[key] = filterForm[key].filter((c) => (c !== item))
  }

  if (filterForm[key].length === 0) {
    delete filterForm[key]
  }
  inventoryState.set({ filterForm })
  formHandler()
}

const filterBadge = (key, count) => {
  if (count === 0) {
    $(`#${key}MobileCount`).remove()
    return
  }

  if ($(`#${key}MobileCount`).length) {
    $(`#${key}MobileCount > span`).text(count)
    return
  }

  $(`#${key}Title > h4`).append(`
    <div id="${key}MobileCount" class="icon-background-color ml-1" style="display: inline-block; vertical-align: middle">
      <span class="font-weight-3 font-size-1 position-absolute absolute-center">${count}</span>
    </div>
  `)
}

const filterList = $('#DesktopFilterList')

const addOtherList = (filter, key, text) => {
  const { aggregatedFilters } = inventoryState.get()
  const isOther = filter.some(item => aggregatedFilters[key].list.includes(item))
  if (isOther) {
    const id = `Other${key}List`
    filterList.append(renderFilterList({

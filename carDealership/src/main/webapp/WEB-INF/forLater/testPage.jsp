function getMinPriceByMonthlyPayment () {
  const { monthlyPayments, filterForm } = inventoryState.get()
  const { downPayment:userDownPayment, tradeIn, paymentMonths, interestRate:userInterestRate, downPaymentPercentage } = monthlyPaymentModal.getMonthlyPaymentData()
  const isDownPaymentPercentage = downPaymentPercentage && downPaymentPercentage > 0
  const downPayment = isDownPaymentPercentage
    ? (downPaymentPercentage / 100)
    : (userDownPayment || monthlyPayments.downPayment)
  const interestRate = (parseFloat(userInterestRate || monthlyPayments.interestPercent) / 100) / 12
  const monthsNumber = parseFloat(paymentMonths || monthlyPayments.months)
  const tradeinValue = parseFloat(tradeIn || monthlyPayments.tradeinValue)

  const mpMin = parseFloat(filterForm.minMonthlyPayment || monthlyPayments.defaultMin)

  if (mpMin !== 0) {
    const x1 = mpMin * (((1 + interestRate) ** monthsNumber) - 1)
    const x2 = interestRate * ((1 + interestRate) ** monthsNumber)
    const x = x1 / x2
    const total = isDownPaymentPercentage
      ? ((x + tradeinValue) / (1 - parseFloat(downPayment)))
      : (x + parseFloat(downPayment) + tradeinValue)
    if (total < 0) {
      return 0
    } else {
      return parseInt(total)
    }
  } else {
    return 0
  }
}

function getMaxPriceByMonthlyPayment () {
  const { defaultFilters, monthlyPayments, filterForm } = inventoryState.get()
  const { downPayment:userDownPayment, tradeIn, paymentMonths, interestRate:userInterestRate, downPaymentPercentage } = monthlyPaymentModal.getMonthlyPaymentData()
  const isDownPaymentPercentage = downPaymentPercentage && downPaymentPercentage > 0
  const downPayment = isDownPaymentPercentage
    ? (downPaymentPercentage / 100)
    : (userDownPayment || monthlyPayments.downPayment)
  const interestRate = (parseFloat(userInterestRate || monthlyPayments.interestPercent) / 100) / 12
  const monthsNumber = parseFloat(paymentMonths || monthlyPayments.months)
  const tradeinValue = parseFloat(tradeIn || monthlyPayments.tradeinValue)

  const mpMax = parseFloat(filterForm.maxMonthlyPayment || monthlyPayments.defaultMax)

  if (mpMax !== 0) {
    const x1 = mpMax * (((1 + interestRate) ** monthsNumber) - 1) 
    const x2 = interestRate * ((1 + interestRate) ** monthsNumber)
    const x = x1 / x2
    const total = isDownPaymentPercentage
      ? ((x + tradeinValue) / (1 - parseFloat(downPayment)))
      : (x + parseFloat(downPayment) + tradeinValue)
    if (total <= 0) {
      return defaultFilters.maxPrice
    } else {
      return Math.min(parseInt(total), defaultFilters.maxPrice)
    }
  } else {
    return defaultFilters.maxPrice
  }
}


function mostlyVisible(element) {
  const windowScrollTop = $(window).scrollTop()
  const windowHeight = $(window).height()
  const windowScrollBottom = windowScrollTop + windowHeight
  const elTop = $(element).offset().top
  const elBottom = elTop + $(element).outerHeight()
  const visibleTop = elTop < windowScrollTop ? windowScrollTop : elTop
  const visibleBottom = elBottom > windowScrollBottom ? windowScrollBottom : elBottom
  const percentage = ((visibleBottom - visibleTop) * 100) / windowHeight

  return percentage >= 50 ? true : false
}

let last_scroll = 0

const changePageQuery = (dataPage) => {
  const queryObject = parseQueryString(window.location.search)
  const targetPage = parseInt(dataPage)
  const currentPage = queryObject.page || 1

  if (currentPage === targetPage || carClicked) {
    return false
  }
  
  if (targetPage == 1) {
    delete queryObject.page
  } else {
    queryObject.page = targetPage
  }
  
  const serializedQueries = serializeQuery(queryObject)
  history.replaceState({ page: targetPage }, null, `?${serializedQueries}`)
  return false
}

const changePageOnScroll = () => {
  const scroll_pos = $(window).scrollTop()
  $(this).removeClass('act')
  if (Math.abs(scroll_pos - last_scroll) > $(window).height() * 0.1) {
    last_scroll = scroll_pos
    $('.carlist-page').each(function(index) {
      if (mostlyVisible(this)) {
        $(this).addClass('act')
        return changePageQuery($(this).attr('data-page'))
      }
    })
  }
}


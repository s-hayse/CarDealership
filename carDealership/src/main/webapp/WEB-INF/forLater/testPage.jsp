function populateResults (resultData, showMissingPages) {
  const monthlyPaymentData = monthlyPaymentModal.getMonthlyPaymentData()
  let htmlResult = ''
  let currentPage = showMissingPages ? 1 : pageIndex
  const currentCount = showMissingPages ? 0 : $('.car-container.search-car-container').length

  resultData.map((car, index) => {
    if (index === 0 || (index % 18) === 0) {
      if (index !== 0) {
        htmlResult += '</div>'
      }
      htmlResult += `<div class="carlist-page row mx-0" id="page-${currentPage}" data-page="${currentPage}">`
      currentPage++
    }

    htmlResult += mapCarDataIntoHtml({ index, car, monthlyPaymentData, currentCount })
  })

  htmlResult += '</div>'

  if (showMissingPages) {
    $('#carList').prepend(htmlResult)
  } else if (pageIndex == 0) {
    $('#carList').html(htmlResult)
  } else {
    $('#carList').append(htmlResult)
  }
  // TODO: Improve this fix
  // Fix change language when we load more cars
  if(getCurrentLanguage() === "es-VE") {
    changeLanguageSpanish();
  }
}

function populateLastCars (resultData) {
  const monthlyPaymentData = monthlyPaymentModal.getMonthlyPaymentData()
  let htmlResult = ''
  const currentCount = (pageIndex - 1) * pageSize - 1

  htmlResult += `
    <div id="loadMissingPages"></div>
    <div class="carlist-page row mx-0" id="page-${(pageIndex - 1)}" data-page="${(pageIndex - 1)}">
  `

  resultData.map((car, index) => {        
    htmlResult += mapCarDataIntoHtml({ index, car, monthlyPaymentData, currentCount }, true)
  })

  htmlResult += '</div>'
  $('#carList').prepend(htmlResult)
}

function queryStringify (removeKeys = []) {
  let { filterForm, filters } = inventoryState.get()
  const finalForm = Object.assign({}, filterForm);

  if (filterForm && !filterForm.orderBy) {
    finalForm.orderBy = filters.orderBy || formDefaults.orderBy
  }

  if (showNoPhotosSection) {
    finalForm.showCarsNoPhotos = true
  }

  if (showPleaseCallSection) {
    finalForm.showPleaseCall  = true
  }

  if (isMonthlyFilterSet()) {
    const { defaultFilters } = inventoryState.get()
    finalForm.minPrice = Math.max(filterForm['minPrice'] || defaultFilters.minPrice, getMinPriceByMonthlyPayment())
    finalForm.maxPrice = Math.min(filterForm['maxPrice'] || defaultFilters.maxPrice, getMaxPriceByMonthlyPayment())
  }

  return serializeQuery(finalForm, removeKeys)
}

function elasticSearchDataNormalization (cars) {
  const newCars = cars.map( carData => {
    const car = carData._source
    return {
      id : carData._id ||  '',
      url: car.carDetailFileName,
      image: car.imageURL || '',
      year: car.year || '',
      make: car.make || '',
      model:car.model || '',
      price: car.priceToBeUsed,
      votes: car.votes ? car.votes : 0,
      type: car.bodytype ? car.bodytype.name : 'TYPE',
      name: car.year + ' ' + car.make + ' ' + car.model + ' ' + car.trim,
      miles: car.miles ? car.miles : 0,
      vin: car.vin ? car.vin : 'VIN',
      additionalImages: [],
      imagesRetrieved: false,
      currentImage: 0,
      interiorColor: car.interiorColor && car.interiorColor !== '' ? car.interiorColor : 'Other',
      inventoryLocation: car.inventoryLocation,
      stockNumber: car.stockNumber,
      vehicleCondition: car.vehicleCondition
    }

  })

  return newCars
}

function updatePageData (result, showMissingPages = false) {
  if (result.total) {
    const carsData = elasticSearchDataNormalization(result.data)
    populateResults(carsData, showMissingPages)
    addAddsToResult()
    setupImageMobileSwipe()
    addCompareCarsBehavior()
    wrapHomenetImages()
    addCarOnClickEvent(showMissingPages)
  }
  else {
    showNoResultsContent(true)
  }
}

async function fetchCarsData () {
  try {
    const queryParmas = queryStringify(['page', 'minMonthlyPayment', 'maxMonthlyPayment'])
    const response = (await http(dealerid, apiUrl).get(serverUrls.searchCars + '?' + queryParmas )).data
    return response
  } catch(err){
    console.log(err)
  }
}

async function searchCars (append, showMissingPages) {
  const result = await fetchCarsData()

  if (result && result.data) {
    const { total, filters } = result
    await updatePageData(result, showMissingPages)
    if (!append) {
      if (result.data.length) {
        await updateAvailableFilters(filters)
      }
      document.dispatchEvent(new CustomEvent('readQueryString'))
      $('#totalMatchesCount, #SidebarTotalMatches').text(`${total} cars shown`)
      changePageMeta(total);

      inventoryState.set({ totalMatchesCount: total })
    }
  }
}

async function subscribeEmail (emailData) {
  try{
    const res = ( await http(dealerid, apiUrl).post(serverUrls.subscribe, { email: emailData, leadFrom: 'SRP' }) )
    if (res.status === 200) {
      return true
    }
    return false
  }
  catch(ex) {
    console.log('Exception subscribing client', ex)
    return false
  }
}


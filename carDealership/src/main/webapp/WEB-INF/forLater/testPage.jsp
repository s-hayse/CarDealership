function isElementInViewport (el) {  // these only consider vertical position
  if (typeof jQuery === "function" && el instanceof jQuery) {
    el = el[0];
  }

  var rect = el.getBoundingClientRect();

  return (
    rect.top >= -30 &&
    rect.bottom <= (window.innerHeight || document.documentElement.clientHeight)
  );
}

function partOfElementIsVisible (el) {  // these only consider vertical position
  if (typeof jQuery === "function" && el instanceof jQuery) {
    el = el[0];
  }

  var rect = el.getBoundingClientRect();
  return !(
    rect.top < 0 && rect.bottom < 0 ||
    rect.top > (window.innerHeight || document.documentElement.clientHeight) && rect.bottom > (window.innerHeight || document.documentElement.clientHeight)
  );
}

const deepEqual = (x, y) => {
  if (x === y) {
    return true
  } else if ((typeof x == "object" && x != null) && (typeof y == "object" && y != null)) {
    if (Object.keys(x).length != Object.keys(y).length) {
      return false
    }

    for (var prop in x) {
      if (y.hasOwnProperty(prop)) {
        if (!deepEqual(x[prop], y[prop])) {
          return false
        }
      }
      else {
        return false
      }
    }
    return true;
  }
  return false
}

const formHandler = (append = false, removeKeys = []) => {
  let newForm = {}
  const { filterForm, defaultFilters, aggregatedFilters } = inventoryState.get()

  Object.keys(filterForm)
    .filter(k => !deepEqual(filterForm[k], defaultFilters[k]))
    .map(k => {
      newForm[k] = filterForm[k]
    })

  Object.keys(newForm)
    .filter(k => Object.keys(aggregatedFilters).includes(k))
    .map((k) => {
      if (newForm[k].includes('Other')) {
        newForm[k] = newForm[k].filter(k => k !== 'Other')
        newForm[k] = newForm[k].concat(aggregatedFilters[k].list)
      }
    })

  coupleFields.forEach((couple) => {
    if (couple[0] in newForm && newForm[couple[0]] !== defaultFilters[couple[0]]) {
      newForm[couple[1]] = newForm[couple[1]] || defaultFilters[couple[1]]
    }
    if (couple[1] in newForm && newForm[couple[1]] !== defaultFilters[couple[1]]) {
      newForm[couple[0]] = newForm[couple[0]] || defaultFilters[couple[0]]
    }
  })

  coupleFields.map(([x, y]) => {
    if (!(x in newForm && y in newForm)) { return }
    const a = parseFloat(newForm[x])
    const b = parseFloat(newForm[y])

    if (!(defaultFilters[x] <= a && a <= defaultFilters[y])) {
      newForm[x] = defaultFilters[x]
    }

    if (!(defaultFilters[x] <= b && b <= defaultFilters[y])) {
      newForm[y] = defaultFilters[y]
    }
  })

  newForm.finish = pageSize

  inventoryState.set({ filterForm: newForm })
  doSearch(append, removeKeys)
}




async function doSearch (append = false, removeKeys = [], updateQuery = true) { 
  const { loading } = inventoryState.get()

  if (loading) {
    return
  } else {
    const { filterForm, totalMatchesCount: matchesCount, defaultFilters } = inventoryState.get()

    let filters = filterForm

    delete filters.interestRate
    delete filters.paymentMonths
    delete filters.tradeIn
    delete filters.downPayment
    delete filters.home

    if (append && filters.start >= matchesCount) {
      return
    }

    // Reset template of cars
    if (!append) {
      $('#carList').html('')
    }

    if (updateQuery) {
      if (!append) {
        pageIndex = 1
        inventoryState.set({ filterForm: Object.assign({}, filters, { start: 0, finish: pageSize }) })

        switchBetweenCategories()
        const removeExtraKeys = addDefaultFormKeys(['start', 'finish', 'interestRate', 'paymentMonths', 'tradeIn', 'home', 'page'])

        const serializedQueries = serializeQuery(filters, removeKeys.concat(removeExtraKeys))
        updateQueryParams(serializedQueries)

        $('html,body').animate({ scrollTop: 0 }, 0)
      } else {
        pageIndex++
      }
    }

    inventoryState.set({ loading: true })

    $('#loadingData').removeClass('d-none')
    $('#allResults').addClass('d-none')
    $('#endBanner').addClass('end-banner')

    await searchCars(append, !updateQuery)

    inventoryState.set({
      loading: false,
      filterForm: Object.assign({}, filterForm, { start: (pageIndex * pageSize) })
    })

    if (!updateQuery) {
      backToPosition()
    }

    showNoResultsBanner()
  }
  stickySidebar.stickySidebar('updateSticky')
}

